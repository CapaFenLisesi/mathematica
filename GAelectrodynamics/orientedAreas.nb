          RowBox[{"AspectRatio", "\[Rule]", "1"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"p", "/.", 
        RowBox[{
         RowBox[{"Line", "[", "x_", "]"}], "\[RuleDelayed]", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"Arrowheads", "[", 
            RowBox[{"{", 
             RowBox[{"0", ",", ".05", 
              RowBox[{"(*", 
               RowBox[{",", ".05"}], "*)"}], ",", "0"}], "}"}], "]"}], ",", 
           RowBox[{"Arrow", "[", "x", "]"}]}], "}"}]}]}]}]}], 
     "\[IndentingNewLine]", "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
